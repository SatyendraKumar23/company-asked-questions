import java.util.*;
public class vowelocccur {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str;
        str=sc.nextLine();
        int count =0;
        for(int i=0;i<str.length();i++)
        {
            char ch=str.charAt(i);
            if(ch=='i' || ch=='o' || ch=='a' || ch=='u' || ch=='e')
            {
                count++;
            }
        }
        System.out.println(count);


    }
    
}
